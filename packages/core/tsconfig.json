{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "baseUrl": "..",
    "paths": {
      "@/contexts/*": ["../web-console/src/contexts/*"],
      "@/lib/*": ["../web-console/src/lib/*"]
    }
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
